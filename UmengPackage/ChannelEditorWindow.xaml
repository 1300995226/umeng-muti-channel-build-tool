<Window x:Class="UmengPackage.ChannelEditorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="编辑渠道" Height="197" Width="400" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid Height="162" Width="379">
        <TextBox Height="77" HorizontalAlignment="Left" Margin="12,41,0,0" Name="channels" VerticalAlignment="Top" Width="352" />
        <TextBlock Height="24" HorizontalAlignment="Left" Margin="12,12,0,0" Text="编辑渠道列表，多渠道用','或';',':'号分隔 Eg: GooglePlay,Appchina " VerticalAlignment="Top" />
        <Button Content="确定" Height="23" HorizontalAlignment="Right" Margin="0,134,16,0" Name="Ok" VerticalAlignment="Top" Width="75" 
                Click="OK_Click" />
        <Button Content="取消" Height="23" HorizontalAlignment="Left" Margin="199,134,0,0" Name="Cancel" VerticalAlignment="Top" Width="75" 
                Click="Cancel_Click"/>
    </Grid>
</Window>
